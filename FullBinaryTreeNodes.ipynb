    "n = 3\n",
    "lst = []\n",
    "\n",
    "a = 2**n\n",
    "lst.append(a)\n",
    "limit = 2 ** (n+1)\n",
    "while a > 1:\n",
    "    x = a//2\n",
    "    while x < limit:\n",
    "        lst.append(x)\n",
    "        x += a\n",
    "    \n",
    "    a = a//2\n",
    "print(\"[\" ,end =\"\")\n",
    "for i in lst:\n",
    "    if i != lst[0]:\n",
    "        print(\",\",end = \"\")\n",
    "    print((i),end = \"\")\n",
    "print(\"]\" ,end =\"\")"
   ]
